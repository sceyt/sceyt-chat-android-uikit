<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SceytAvatarView">
        <attr name="sceytAvatarViewImageUrl" format="string" />
        <attr name="sceytAvatarViewFullName" format="string" />
        <attr name="sceytAvatarViewIsGroup" format="boolean" />
        <attr name="sceytAvatarViewTextSize" format="dimension|integer" />
        <attr name="sceytAvatarColor" format="color|reference" />
        <attr name="sceytAvatarDefaultIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="SceytToReplyLineView">
        <attr name="sceytToReplyLineViewToLeft" format="boolean" />
        <attr name="sceytToReplyLineViewStrokeColor" format="reference|color" />
        <attr name="sceytToReplyLineViewConnectedViewTop" format="reference" />
