<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AvatarView">
        <attr name="sceytUiAvatarFullName" format="string" />

        <!--Text style -->
        <attr name="sceytUiAvatarTextSize" format="dimension|integer" />
        <attr name="sceytUiAvatarTextColor" format="color|reference" />
        <attr name="sceytUiAvatarTextFont" format="reference" />
        <attr name="sceytUiAvatarTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>

        <attr name="sceytUiAvatarColor" format="color|reference" />
        <attr name="sceytUiAvatarDefaultIcon" format="reference" />
        <attr name="sceytUiAvatarEnableRipple" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ToReplyLineView">
        <attr name="sceytUiToReplyLineToLeft" format="boolean" />
        <attr name="sceytUiToReplyLineStrokeColor" format="reference|color" />
        <attr name="sceytUiToReplyLineConnectedViewTop" format="reference" />
        <attr name="sceytUiToReplyLineConnectedViewBottom" format="reference" />
    </declare-styleable>

    <declare-styleable name="DecoratedTextView">
        <attr name="sceytUiDecoratedTextLeadingIcon" format="reference" />
        <attr name="sceytUiDecoratedTextTrailingIcon" format="reference" />
        <attr name="sceytUiDecoratedTextLeadingIconSize" format="dimension" />
        <attr name="sceytUiDecoratedTextTrailingIconSize" format="dimension" />
        <attr name="sceytUiDecoratedTextIconsTint" format="reference|color" />
        <attr name="sceytUiDecoratedTextLeadingIconPadding" format="dimension" />
        <attr name="sceytUiDecoratedTextTrailingIconPadding" format="dimension" />
        <!--Text style-->
        <attr name="sceytUiDecoratedTextTitle" format="string" />
        <attr name="sceytUiDecoratedTextSize" format="dimension" />
        <attr name="sceytUiDecoratedTextColor" format="reference|color" />
        <attr name="sceytUiDecoratedTextFont" format="reference" />
        <attr name="sceytUiDecoratedTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>

        <!--Leading text style-->
        <attr name="sceytUiDecoratedTextLeadingText" format="string" />
        <attr name="sceytUiDecoratedTextLeadingTextSize" format="dimension" />
        <attr name="sceytUiDecoratedTextLeadingTextColor" format="reference|color" />
        <attr name="sceytUiDecoratedTextLeadingTextFont" format="reference" />
        <attr name="sceytUiDecoratedTextLeadingTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>

        <attr name="sceytUiDecoratedTextIgnoreRtl" format="boolean" />
        <attr name="sceytUiDecoratedTextBackgroundColor" format="color|reference" />
        <attr name="sceytUiDecoratedTextBackgroundCornerRadius" format="dimension" />
        <attr name="sceytUiDecoratedTextHighlighted" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ReactionView">
        <attr name="sceytUiReactionInnerPadding" format="integer|dimension" />
        <attr name="sceytUiReactionInnerPaddingHorizontal" format="integer|dimension" />
        <attr name="sceytUiReactionInnerPaddingVertical" format="integer|dimension" />
        <attr name="sceytUiReactionSmileTextSize" format="integer|dimension" />
        <attr name="sceytUiReactionCountTextSize" format="integer|dimension" />
        <attr name="sceytUiReactionCountTextColor" format="color|reference" />
        <attr name="sceytUiReactionCountTextFont" format="reference" />
        <attr name="sceytUiReactionCountTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="sceytUiReactionCountTextMargin" format="integer|dimension" />
        <attr name="sceytUiReactionEnableStroke" format="boolean" />
        <attr name="sceytUiReactionStrokeColor" format="color|reference" />
        <attr name="sceytUiReactionStrokeWidth" format="integer|dimension" />
        <attr name="sceytUiReactionStrokeCornerRadius" format="integer|dimension" />
        <attr name="sceytUiReactionCountText" format="string" />
        <attr name="sceytUiReactionSmileText" format="string" />
        <attr name="sceytUiReactionBackgroundColor" format="color|reference" />
        <attr name="sceytUiReactionCountTextMinWidth" format="integer|dimension" />
    </declare-styleable>

    <declare-styleable name="VideoControllerView">
        <attr name="sceytUiVideoControllerShowPlayPause" format="boolean" />
        <attr name="sceytUiVideoControllerPlayPauseSize" format="integer|dimension" />
        <attr name="sceytUiVideoControllerPlayIcon" format="reference" />
        <attr name="sceytUiVideoControllerPauseIcon" format="reference" />
        <attr name="sceytUiVideoControllerImage" format="reference" />
        <attr name="sceytUiVideoControllerCornerSize" format="dimension" />
        <attr name="sceytUiVideoControllerEnablePlayPauseClick" format="boolean" />

    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="sceytUiProgressColor" format="color" />
        <attr name="sceytUiProgressTrackColor" format="color" />
        <attr name="sceytUiProgressValue" format="float" />
        <attr name="sceytUiProgressMinProgress" format="float" />
        <attr name="sceytUiProgressRoundedProgress" format="boolean" />
        <attr name="sceytUiProgressTrackThickness" format="dimension" />
        <attr name="sceytUiProgressCenterIcon" format="reference" />
        <attr name="sceytUiProgressIconTint" format="color" />
        <attr name="sceytUiProgressRotateAnimEnabled" format="boolean" />
        <attr name="sceytUiProgressBackgroundColor" format="color" />
        <attr name="sceytUiProgressIconSizeInPercent" format="float" />
        <attr name="sceytUiProgressEnableProgressDownAnimation" format="boolean" />

    </declare-styleable>

    <declare-styleable name="CustomToolbar">
        <attr name="sceytUiToolbarTitle" format="string" localization="suggested" />
        <attr name="sceytUiToolbarSubtitle" format="string" localization="suggested" />
        <attr name="sceytUiToolbarTitleTextColor" format="color|reference" />
        <attr name="sceytUiToolbarSubtitleTextColor" format="color|reference" />
        <attr name="sceytUiToolbarNavigationIcon" format="reference" />
        <attr name="sceytUiToolbarMenu" format="reference" />
        <attr name="sceytUiToolbarIconsTint" format="color|reference" />
        <attr name="sceytUiToolbarEnableDivider" format="boolean" />
        <attr name="sceytUiToolbarDividerColor" format="color|reference" />
    </declare-styleable>

</resources>