        <attr name="sceytVideoControllerPauseIcon" format="reference" />

    </declare-styleable>

    <declare-styleable name="SceytCircularProgressView">
        <attr name="progressColor" format="color"/>
        <attr name="trackColor" format="color"/>
        <attr name="progress" format="float"/>
        <attr name="minProgress" format="float"/>
        <attr name="roundedProgress" format="boolean"/>
        <attr name="trackThickness" format="dimension"/>
        <attr name="centerIcon" format="reference"/>
        <attr name="iconTint" format="color"/>
        <attr name="rotateAnimEnabled" format="boolean"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="iconSizeInPercent" format="float"/>

    </declare-styleable>

    <declare-styleable name="SceytCustomToolbar">
        <attr name="title" format="string" />
        <attr name="navigationIcon" format="reference" />
        <attr name="navigationEndIcon" format="reference" />
    </declare-styleable>

