        <attr name="sceytReactionViewStrokeColor" format="color|reference" />
        <attr name="sceytReactionViewStrokeWidth" format="integer|dimension" />
        <attr name="sceytReactionViewStrokeCornerRadius" format="integer|dimension" />
        <attr name="sceytReactionViewCountText" format="string|reference" />
        <attr name="sceytReactionViewSmileText" format="string|reference" />
        <attr name="sceytReactionViewBackgroundColor" format="color|reference" />
        <attr name="sceytReactionViewCountTextMinWidth" format="integer|dimension" />
